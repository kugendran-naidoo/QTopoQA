# Inference configuration for dynamic features

# Input/output paths resolved relative to this YAML
paths:
  data_dir: ../../../datasets/training/adjusted/pilot_batch_Dockground_MAF2
  label_file: ../../../datasets/training/adjusted/pilot_batch_Dockground_MAF2/label_info_Dockground_MAF2.csv
  work_dir: ./output/pilot_batch/work
  results_dir: ./output/pilot_batch/results
  dataset_name: pilot_batch_Dockground_MAF2
  checkpoint: null

batch_size: 32
num_workers: 0

# Builder options (use training metadata for feature layout; only jobs typically need adjustment)
builder:
  jobs: 12

# Optional run-time behaviour toggles
options:
  reuse_existing_graphs: true
