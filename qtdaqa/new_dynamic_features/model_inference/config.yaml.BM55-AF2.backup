# Inference configuration for dynamic features

# Input/output paths resolved relative to this YAML
paths:
  data_dir: ../../../datasets/evaluation/BM55-AF2/decoy
  label_file: ../../../datasets/evaluation/BM55-AF2/label_info.csv
  work_dir: ./output/11d_topoqa_10.5A/BM55-AF2/work
  results_dir: ./output/11d_topoqa_10.5A/BM55-AF2/results
  dataset_name: BM55-AF2
  checkpoint: null

batch_size: 32
num_workers: 0

# Builder options (use training metadata for feature layout; only jobs typically need adjustment)
builder:
  jobs: 12

# Optional run-time behaviour toggles
options:
  reuse_existing_graphs: true
